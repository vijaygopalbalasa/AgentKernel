{
  "name": "@agentkernel/adapter-openclaw",
  "version": "0.1.0",
  "description": "AgentKernel adapter for running OpenClaw agents inside a sandboxed runtime",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "lint": "biome check src/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "agentkernel",
    "openclaw",
    "adapter",
    "ai-agents",
    "sandbox"
  ],
  "dependencies": {
    "@agentkernel/runtime": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0"
  }
}
